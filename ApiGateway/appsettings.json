{
  "ReverseProxy": {
    "Routes": {
      "orders_route": {
        "ClusterId": "orders_cluster",
        "Match": {
          "Path": "/api/orders/{**catch-all}"
        }
      },
      "accounts_route": {
        "ClusterId": "payments_cluster",
        "Match": {
          "Path": "/accounts/{**catch-all}"
        }
      },
      "hubs_route": {
        "ClusterId": "orders_cluster",
        "Match": {
          "Path": "/hubs/{**catch-all}"
        }
      },
      "orders_swagger": {
        "ClusterId": "orders_cluster",
        "Match": {
          "Path": "/swagger/orders/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/swagger/orders" }
        ]
      },
      "payments_swagger": {
        "ClusterId": "payments_cluster",
        "Match": {
          "Path": "/swagger/payments/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/swagger/payments" }
        ]
      }
    },
    "Clusters": {
      "orders_cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "payments_cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5002/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
